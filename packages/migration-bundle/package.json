{
	"name": "@directus-labs/migration-bundle",
	"type": "module",
	"version": "1.0.0",
	"description": "Migrate from the current source Directus instance to another target Directus instance.",
	"license": "MIT",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-bundle"
	],
	"icon": "extension",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "module",
				"name": "migration-module",
				"source": "src/migration-module/index.ts"
			},
			{
				"type": "endpoint",
				"name": "migration-endpoint",
				"source": "src/migration-endpoint/index.ts"
			}
		],
		"host": "^11.1.2"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"add": "directus-extension add",
		"validate": "directus-extension validate"
	},
	"dependencies": {
		"@directus/sdk": "^19.0.1",
		"@directus/utils": "^13.0.0",
		"bottleneck": "^2.19.5",
		"defu": "^6.1.4",
		"dompurify": "^3.2.4",
		"formdata-node": "^6.0.3",
		"marked": "^15.0.7"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "13.0.1",
		"@directus/types": "^13.0.0",
		"@types/node": "^22.13.4",
		"typescript": "^5.7.3",
		"vue": "^3.5.13"
	}
}
