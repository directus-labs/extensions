{
	"name": "@directus-labs/video-player-interface",
	"type": "module",
	"version": "1.0.1",
	"description": "An interface to select and display a video from YouTube, Vimeo or a local file from Directus",
	"author": "Directus Labs",
	"contributors": [
		{
			"name": "formfcw",
			"email": "dev@formfcw.com"
		}
	],
	"license": "MIT",
