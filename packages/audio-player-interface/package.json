{
    "name": "directus-extension-audio-interface",
    "version": "1.0.0",
    "description": "An interface to select an audio source and display an audio player from an URL or a local file from Directus",
    "license": "MIT",
    "author": "Directus Labs",
    "contributors": [
        {
            "name": "formfcw",
            "email": "dev@formfcw.com"
        }
    ],
    "icon": "play_circle",
    "keywords": [
        "mp3",
        "audio-player",
        "audio",
        "audio-extension",
        "audio-select",
        "inline-audio",
        "directus",
        "directus-extension",
        "directus-extension-interface"
    ],
    "type": "module",
    "files": [
        "dist"
    ],
    "directus:extension": {
        "type": "interface",
        "path": "dist/index.js",
        "source": "src/index.ts",
        "host": "^10.10.0"
    },
    "scripts": {
        "build": "directus-extension build",
        "dev": "directus-extension build -w --no-minify",
        "link": "directus-extension link"
    },
    "devDependencies": {
        "@directus/extensions-sdk": "^11.0.9",
        "typescript": "^5.4.5",
        "vue": "^3.4.27",
        "vue-i18n": "^9.13.1"
    },
    "dependencies": {
        "plyr": "^3.7.8"
    }
}
