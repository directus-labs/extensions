{
	"name": "@directus-labs/gantt-chart-layout",
	"type": "module",
	"version": "1.0.1",
	"description": "Display items in a gantt chart to plot timelines and dependencies.",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-layout"
	],
	"icon": "extension",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "layout",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^10.0.0 || ^11.0.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"dependencies": {
		"sass": "^1.77.8"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "12.0.0",
		"typescript": "^5.5.4",
		"vue": "^3.4.36"
	}
}
