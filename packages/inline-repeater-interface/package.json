{
	"name": "@directus-labs/inline-repeater-interface",
	"type": "module",
	"version": "1.0.1",
	"description": "A powerful interface for managing repeatable form fields within Directus that allows inline editing and reordering of items.",
	"license": "MIT",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-interface"
	],
	"icon": "extension",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "interface",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^10.0.0 || ^11.0.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate"
	},
	"dependencies": {
		"@directus/format-title": "12.1.0",
		"lodash": "4.17.21",
		"reka-ui": "2.5.0",
		"sass": "1.77.8",
		"vue": "3.5.18",
		"vue-i18n": "11.1.11",
		"vuedraggable": "4.1.0"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "16.0.2",
		"@directus/types": "^13.0.0",
		"@types/lodash": "^4.17.20",
		"typescript": "^5.5.4"
	}
}
