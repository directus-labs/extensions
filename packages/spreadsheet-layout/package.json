{
	"name": "@directus-labs/spreadsheet-layout",
	"type": "module",
	"version": "1.0.2",
	"description": "A spreadsheet layout that allows you to edit item fields directly inline, just like in a spreadsheet",
	"author": "Directus Labs",
	"contributors": [
		{
			"name": "formfcw",
			"email": "dev@formfcw.com"
		}
	],
	"license": "MIT",
	"keywords": [
		"inline",
		"inline-editing",
		"spreadsheet",
		"directus",
		"directus-extension",
		"directus-extension-layout",
		"directus-extension-spreadsheet-layout"
	],
	"icon": "table",
	"files": [
		"dist"
	],
